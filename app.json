{
  "expo": {
    "name": "Passeio",
    "slug": "Passeio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconPasseio.png",
    "splash": {
      "image": "./assets/logoSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jabdo89.Passeio"
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.jabdo89.Passeio"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
